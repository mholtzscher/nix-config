float alpha_word_p(in vec2 p, in float aa) {
  if (p.x < 70.6 - aa || p.x > 95.7001 + aa || p.y < 25.39 - aa || p.y > 60.9 + aa) return 0.0;
  float wn = 0.0;
  float dmin = 1e9;
  for (int i = 0; i < word_p_N; i++) {
    vec2 a = word_p[i].xy;
    vec2 b = word_p[i].zw;
    // Distance to edge
    vec2 pa = p - a;
    vec2 ba = b - a;
    float h = clamp(dot(pa, ba) / dot(ba, ba), 0.0, 1.0);
    dmin = min(dmin, length(pa - ba * h));

    // Nonzero winding number via ray casting (to +X)
    bool cond = (a.y <= p.y && b.y > p.y) || (a.y > p.y && b.y <= p.y);
    if (cond) {
      float x = a.x + (p.y - a.y) * (b.x - a.x) / (b.y - a.y);
      if (x > p.x) wn += (b.y > a.y) ? 1.0 : -1.0;
    }
  }

  float sd = (abs(wn) > 0.5) ? -dmin : dmin;
  // Hard edge fill (no AA) to avoid halo/border.
  return (abs(wn) > 0.5) ? 1.0 : 0.0;
}


  a = alpha_word_i_dot(pSvg, aa);  acc = overPremul(acc, vec4(PAYTIENT_PURPLE * a, a));
  a = alpha_word_e(pSvg, aa);      acc = overPremul(acc, vec4(PAYTIENT_PURPLE * a, a));
  a = alpha_word_n(pSvg, aa);      acc = overPremul(acc, vec4(PAYTIENT_PURPLE * a, a));
  a = alpha_word_t2(pSvg, aa);     acc = overPremul(acc, vec4(PAYTIENT_PURPLE * a, a));

  // Mark layers (two are 0.6 opacity)
  a = alpha_mark_top(pSvg, aa) * 0.6;    acc = overPremul(acc, vec4(PAYTIENT_PURPLE * a, a));
  a = alpha_mark_bottom(pSvg, aa) * 0.6; acc = overPremul(acc, vec4(PAYTIENT_PURPLE * a, a));
  a = alpha_mark_main(pSvg, aa);         acc = overPremul(acc, vec4(PAYTIENT_PURPLE * a, a));

  return acc;
}

void mainImage(out vec4 fragColor, in vec2 fragCoord) {
  fragColor = texture(iChannel0, fragCoord.xy / iResolution.xy);

  // Keep the SVG's aspect ratio (252:80 = 3.15).
  float logoHeightPx = min(iResolution.y * 0.16, (iResolution.x / 3.15) * 0.92);
  float logoWidthPx = logoHeightPx * 3.15;

  vec2 logoSizePx = vec2(logoWidthPx, logoHeightPx);
  vec2 pad = vec2(10.0, 10.0);

  vec2 minCenter = logoSizePx * 0.5 + pad;
  vec2 maxCenter = iResolution.xy - logoSizePx * 0.5 - pad;
  vec2 rangePx = max(maxCenter - minCenter, vec2(1.0));

  // DVD-style ping-pong bounce.
  float vx = 160.0;
  float vy = 130.0;

  float timeX = iTime * vx;
  float timeY = iTime * vy;

  float pingX = abs(mod(timeX, 2.0 * rangePx.x) - rangePx.x);
  float pingY = abs(mod(timeY, 2.0 * rangePx.y) - rangePx.y);

  vec2 centerPx = minCenter + vec2(pingX, pingY);

  // Ghostty is Shadertoy-like, but coordinate origin can vary by build.
  // `FRAGCOORD_Y_DOWN = 0.0` assumes Shadertoy-style (y-up, origin bottom-left).
  // If your build behaves like y-down (origin top-left), set it to 1.0.
  const float FRAGCOORD_Y_DOWN = 1.0;

  vec2 fragYDown = vec2(fragCoord.x, mix(iResolution.y - fragCoord.y, fragCoord.y, FRAGCOORD_Y_DOWN));
  vec2 centerYDown = vec2(centerPx.x, mix(iResolution.y - centerPx.y, centerPx.y, FRAGCOORD_Y_DOWN));
  vec2 topLeftYDown = centerYDown - logoSizePx * 0.5;

  vec2 localPx = fragYDown - topLeftYDown;
  if (any(lessThan(localPx, vec2(0.0))) || any(greaterThan(localPx, logoSizePx))) {
    return;
  }

  // Map to SVG coordinate space (0..252, 0..80), y-down.
  vec2 pSvg = vec2(
    localPx.x * (252.0 / logoSizePx.x),
    localPx.y * (80.0 / logoSizePx.y)
  );

  // AA disabled: pass 0 (kept for API shape).
  float aa = 0.0;

  vec4 logo = drawPaytientLogo(pSvg, aa);

  // Composite premultiplied logo over the terminal.
  vec3 outRgb = logo.rgb + fragColor.rgb * (1.0 - logo.a);
  fragColor = vec4(outRgb, fragColor.a);
}
